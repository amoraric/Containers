{
    "manifest_version": 2,
    "name": "Tab Groups Sidebar Extension",
    "version": "1.0",
    "description": "An extension that manages tab groups.",
    "permissions": [
      "tabs",
      "storage",
      "downloads"
    ],
    "sidebar_action": {
      "default_title": "Tab Groups",
      "default_panel": "sidebar.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png"
      }
    },
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png"
      },
      "default_title": "Toggle Sidebar"
    },
    "background": {
      "scripts": ["background.js"]
    }
  }
  