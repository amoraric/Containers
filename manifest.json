{
    "manifest_version": 2,
    "name": "Tab Containers",
    "version": "1.0",
    "description": "Personal extension that manages tab containers",
    "permissions": [
      "tabs",
      "storage",
      "downloads",
      "activeTab",
      "<all_urls>"
    ],
    "sidebar_action": {
      "default_title": "Containers",
      "default_panel": "sidebar.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png"
      }
    },
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png"
      },
      "default_title": "Toggle Sidebar"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "commands": {
      "_execute_sidebar_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "Toggle Tab Groups Sidebar"
      }
    }
  }
  